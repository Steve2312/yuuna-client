  "devDependencies": {
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@vitejs/plugin-react": "^1.2.0",
    "dotenv": "^16.0.0",
    "electron": "^13.6.9",
    "electron-builder": "^22.14.13",
    "execa": "^6.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.49.9",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "vite-plugin-esmodule": "^1.2.5",
