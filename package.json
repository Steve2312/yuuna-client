{
  "name": "yuuna-client",
  "productName": "yuuna-client",
  "main": "./dist/main.bundle.js",
  "version": "1.0.0",
  "description": "Yuuna is a music player that uses the Beatconnect API to download osu! beatmaps.",
  "author": "Steve2312",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "cross-env NODE_ENV=production webpack --config config/webpack.config.js",
    "compile_main": "cross-env NODE_ENV=production webpack --config config/webpack.main.config.js",
    "compile_renderer": "cross-env NODE_ENV=production webpack --config webpack.renderer.config.js.config.js",
    "serve": "cross-env NODE_ENV=development webpack serve --hot --config config/webpack.config.js",
    "dev": "npm run compile_main && cross-env DEV_HOST=localhost DEV_PORT=8080 node scripts/start.js",
    "build": "npm run compile && electron-builder --config config/electron-builder-config.js"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^18.0.7",
    "@types/react-dom": "^18.0.0",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.0",
    "electron": "^13.6.9",
    "electron-builder": "^23.0.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "sass": "^1.50.1",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "wait-on": "^6.0.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "axios": "^0.27.0",
    "react-icons": "^4.3.1",
    "webpack-dev-server": "^4.8.1"
  }
}
